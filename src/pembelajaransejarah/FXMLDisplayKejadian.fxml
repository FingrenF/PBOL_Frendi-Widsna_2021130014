<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="687.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pembelajaransejarah.FXMLDisplayKejadianController">
   <children>
      <TableView fx:id="tbvkejadian" layoutX="14.0" layoutY="105.0" onMouseClicked="#tableklik" prefHeight="232.0" prefWidth="435.0">
         <columns>
            <TableColumn prefWidth="75.0" text="C1" />
            <TableColumn prefWidth="75.0" text="C2" />
         </columns>
      </TableView>
      <Button fx:id="btntambah" layoutX="14.0" layoutY="346.0" mnemonicParsing="false" onAction="#tambahklik" text="+" />
      <Button fx:id="btnhapus" layoutX="80.0" layoutY="346.0" mnemonicParsing="false" onAction="#hapusklik" text="-" />
      <Button fx:id="btnubah" layoutX="149.0" layoutY="346.0" mnemonicParsing="false" onAction="#ubahklik" text="Edit" />
      <Button fx:id="btnawal" layoutX="216.0" layoutY="346.0" mnemonicParsing="false" onAction="#awalklik" text="|&lt;" />
      <Button fx:id="btnsebelum" layoutX="284.0" layoutY="346.0" mnemonicParsing="false" onAction="#sebelumklik" text="&lt;" />
      <Button fx:id="btnsesudah" layoutX="351.0" layoutY="347.0" mnemonicParsing="false" onAction="#sesudahklik" text="&gt;" />
      <Button fx:id="btnakhir" layoutX="414.0" layoutY="347.0" mnemonicParsing="false" onAction="#akhirklik" text="&gt;|" />
      <Label layoutX="185.0" layoutY="24.0" prefHeight="44.0" prefWidth="356.0" text="Daftar Nama Kejadian">
         <font>
            <Font name="Bell MT Bold" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="searchbar" layoutX="16.0" layoutY="68.0" onKeyReleased="#cariData" prefHeight="25.0" prefWidth="435.0" promptText="Search" />
      <TextArea fx:id="txadeskripsi" layoutX="459.0" layoutY="105.0" prefHeight="232.0" prefWidth="200.0" />
   </children>
</AnchorPane>
